<section-cc id="cc">
  <div class="container cc">
    <div class="row cc-flex">
      <div class="col-lg-6 col-md-6">
        <div class="cc-caption">
          <p>Course Content</p>
        </div>
        <div class="cc-body">
          <h2 class="fs1 fw-bold mb-5">Enroll into <br> Digital Marketing Course Programme</h2>
          <p>Master the basics of digital marketing with our advanced and industry-recognized marketing methods.</p>
          <div class="cc-link">
            <a data-bs-toggle="modal" data-bs-target="#brochure" class="fs-5">Download Brochure <i class="fa-solid fa-arrow-right"></i></a>
          </div>
            <!-- Modal -->
          <div class="modal fade" id="brochure" tabindex="-1" aria-labelledby="brochure" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered">
              <div class="modal-content align-items-center">
                <div class="modal-header">
                  <h4 class="modal-title text-center" id="brochure">Download <span>Brochure</span></h4>
                </div>
                <div class="modal-body">
                  <form id="contact-form" action="/" method="post">
                    <div class="row">
                      <div class="col-lg-6">
                        <fieldset>
                          <input type="name" name="name" id="name" placeholder="Your Full Name..." autocomplete="on" required>
                        </fieldset>
                      </div>
                      <div class="col-lg-6">
                        <fieldset>
                          <input type="phone" name="Phone" id="Phone" pattern="[6-9]{1}[0-9]{9}" placeholder="Phone Number..." autocomplete="on" required>
                        </fieldset>
                      </div>
                      <div class="col-lg-12">
                        <fieldset>
                          <input type="text" name="email" id="email" pattern="[^ @]*@[^ @]*" placeholder="Your E-mail..." required="">
                        </fieldset>
                      </div>
                      <div class="col-lg-12">
                        <p class="brochure-note">**Brochure will be sent to you given mail id.</p>
                      </div>
                      <div class="col-lg-12">
                        <fieldset>
                          <button data-ani="bounceOutRight" type="submit" id="form-submit" class="orange-button">Submit</button>
                        </fieldset>
                      </div>
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-lg-6 col-md-6">
        <img src="assets/images/cc-section.webp" alt="Digital Marketing Course 7" class="img-fluid">
      </div>
    </div>

    <div class="row cc-grid">
     <% slides.forEach(function(slide,i) { %>
      <div class="col-lg-2 col-md-3 col-sm-6 col-xs-6 col-6">
        <div class="cc-grid-inner">
          <img src="<%=slideImg[i]%>" alt="<%=slide%>" class="img-fluid">
          <h3><%=slide%></h3>
          <!-- <p class="wcu-p">Autopublish your posts, carousels, and Reels directly to Instagram</p> -->
        </div>
      </div>
      <% }) %>
    </div>
  </div>
</section-cc>

